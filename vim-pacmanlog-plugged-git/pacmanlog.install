_gitname=pacmanlog.vim

post_install() {
  printf "$pacmanlog\n"
}

post_upgrade() {
  post_install
}

post_remove() {
  echo -n "Removing $_gitname directory... "
  rm -rf "$HOME/.vim/plugged/$_gitname"
  echo "done"
}

read -d '' pacmanlog <<'EOF'
# vim-pacmanlog-git

Highlight the time, different actions, warnings and errors in Pacman
log files.
EOF
